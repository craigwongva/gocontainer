sudo yum install docker -y > /tmp/4100 2>&1
sudo service docker start > /tmp/4200 2>&1
#The following is necessary if working interactively as ec2-user:
# sudo usermod -a -G docker ec2-user

cd /home/ec2-user/gocontainer/nexus-java > /tmp/4300 2>&1
#sudo cp Dockerfile-first_webapp Dockerfile #this starts 8080 for test purposes but isn't central to docker+nexus issue
sudo cp Dockerfile-nexus-servicecontroller Dockerfile > /tmp/4400 2>&1
sudo docker build --build-arg WHEREISNEXUS=$1 -t venicegeodev/container_servicecontroller . > /tmp/4500 2>&1
sudo docker login --username venicegeodev --password demo1024 > /tmp/4510 2>&1
sudo docker push venicegeodev/container_servicecontroller > /tmp/4520 2>&1
sudo docker pull venicegeodev/container_servicecontroller> /tmp/4530 2>&1
sudo docker run -d -p 8888:8088 venicegeodev/container_servicecontroller > /tmp/4600 2>&1





