cd ~ > /tmp/userdata-groovy-lf-1400 2>&1
git clone https://github.com/craigwongva/3deploy.git > /tmp/userdata-groovy-lf-1500 2>&1
cp ~/3deploy/userdata-logstashforwarder . > /tmp/userdata-groovy-lf-1600 2>&1
