sudo yum install docker -y
sudo service docker start
#The following is necessary if working interactively as ec2-user:
# sudo usermod -a -G docker ec2-user
git clone https://github.com/craigwongva/gocontainer
mkdir /gocontainerbuilder
cp gocontainer/Dockerfile-first_webapp gocontainerbuilder/Dockerfile
cd /gocontainerbuilder
docker build -t first_webapp .
docker run -d -p 8080:8080 first_webapp

