sudo yum install docker -y
sudo service docker start
#The following is necessary if working interactively as ec2-user:
# sudo usermod -a -G docker ec2-user

cd ~/gocontainer
sudo cp Dockerfile-first_webapp Dockerfile #this starts 8080 for test purposes but isn't central to docker+nexus issue
sudo cp Dockerfile-nexus-first_webapp Dockerfile
sudo docker build --build-arg WHEREISNEXUS=$1 -t container_first_webapp .
#sudo docker run -d -p 8080:8080 container_first_webapp





